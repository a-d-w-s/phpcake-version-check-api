{
    "name": "shadowx-jb/phpcake-version-check-api",
    "description": "VersionCheck plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "VersionCheck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VersionCheck\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
